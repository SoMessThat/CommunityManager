<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mess.wkb.cm.code.dao.CmAttendanceDAO" >
	<resultMap id="CmAttendanceResultMap" type="mess.wkb.cm.code.po.CmAttendancePO">
		<result property="id" column="id" />  
		<result property="name" column="name" />  
		<result property="creatTime" column="creat_time" />  
		<result property="beginTime" column="begin_time" />  
		<result property="endTime" column="end_time" />  
		<result property="departmentId" column="department_id" />  
		<result property="userName" column="user_name" />  
		<result property="state" column="state" />  
	</resultMap>
	
	
	
	<!-- 签到会议  -->
	<insert id="addCmAttendance" keyColumn="id" keyProperty="id" parameterType="mess.wkb.cm.code.po.CmAttendancePO" useGeneratedKeys="true">
	    insert into cm_attendance
	    <trim prefix="(" suffix=")" suffixOverrides=",">
	      <if test="name != null">			   name, </if>
	      <if test="creatTime != null">        creatTime,      </if>
	      <if test="beginTime != null">        beginTime,      </if>
	      <if test="endTime != null">          endTime,      </if>
	      <if test="departmentId != null">     departmentId,      </if>
	      <if test="userName != null">         userName,      </if>
	      <if test="state != null">            state,      </if>
	    </trim>
	    <trim prefix="values (" suffix=")" suffixOverrides=",">
	      <if test="name != null">             #{name},      </if>
	      <if test="creatTime != null">        #{creatTime},      </if>
	      <if test="beginTime != null">        #{beginTime},      </if>
	      <if test="endTime != null">          #{endTime},      </if>
	      <if test="departmentId != null">     #{departmentId},      </if>
	      <if test="userName != null">         #{userName},      </if>
	      <if test="state != null">       	   #{state},      </if>
	    </trim>
  </insert>
  
  
  
  
  
  
  
  
</mapper>  
